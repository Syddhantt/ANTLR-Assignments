begin
	integer v
	integer x
	let x=1
	print "A="
	input v
	function iterativeFactorial(in integer val) : integer
	begin 
		integer b,c
		let c = 1
		for b=1 to b<val step 1
		begin  
		  let c = b * c
		end
		return c
	end
		
	function recursiveFactorial(in integer j) : integer
	begin	
		let x=x*j
		if j>1
		begin
			recursiveFactorial(j-1)
		end
	return x
	end
	print "Iteractive Factorial: ", iterativeFactorial(v)  
	print "Recursive Factorial: ", recursiveFactorial(v)  
end